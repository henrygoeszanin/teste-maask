# Server Configuration
PORT=3000
HOST=0.0.0.0
NODE_ENV=development

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/teste_maask

# Security - Argon2
PEPPER="your_secure_pepper_here_min_32_chars"
PEPPER_VERSION=1
ARGON2_MEMORY_COST=15360
ARGON2_TIME_COST=2
ARGON2_PARALLELISM=1

# JWT Authentication
JWT_SECRET="your_jwt_secret_here_min_32_chars"
JWT_REFRESH_SECRET="your_jwt_refresh_secret_here_min_32_chars"
JWT_ACCESS_EXPIRES_IN=900
JWT_REFRESH_EXPIRES_IN=2592000

# Supabase Configuration
SUPABASE_URL="https://your-project.supabase.co"
SUPABASE_ANON_KEY="your_supabase_anon_key"
SUPABASE_SERVICE_ROLE_KEY="your_supabase_service_role_key"
SUPABASE_STORAGE_BUCKET="user-data"

# CORS Configuration
CORS_ORIGIN="http://localhost:5173"

# Rate Limiting
RATE_LIMIT_GLOBAL_MAX=100
RATE_LIMIT_GLOBAL_TIME_WINDOW=900000
RATE_LIMIT_AUTH_MAX=5
RATE_LIMIT_AUTH_TIME_WINDOW=900000
RATE_LIMIT_UPLOAD_MAX=10
RATE_LIMIT_UPLOAD_TIME_WINDOW=3600000
